{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es2015",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "vue":["../node_modules/vue"]
    }, "outDir": "dist",
    "removeComments": true,
    "declaration": true,
  },
  "exclude": [
      "bin",
      "node_modules"
  ], "files": [
    "index.ts"
  ]
}
